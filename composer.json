{
    "name": "ava007/wnk_translation",
    "description": "Translation plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "WnkTranslation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WnkTranslation\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
